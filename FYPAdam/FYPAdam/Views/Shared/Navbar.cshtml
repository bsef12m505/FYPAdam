    <script type="text/javascript">


        function TitleIntellisecce() {
            $.getScript('../../Scripts/jquery-ui.js');
            var titles = [];
            $.ajax({
                url: '../../ADAMHome/AllProductTtiles',
                type: 'Get',
                dataType: 'json',
                success: function (data) {

                    $(data).each(function (index, item) {
                        titles.push(item);
                    });
                },
                error: function () {
                    alert('error');
                }
            });

            $("#searchTextBox").autocomplete({
                source: titles
            });
        }
    </script>

