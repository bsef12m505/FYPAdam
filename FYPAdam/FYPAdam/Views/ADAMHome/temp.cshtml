            var titles = [];
            $.ajax({
                url: 'http://localhost/Engine/Home/AllProductTtiles',
                type: 'Get',
                dataType: 'json',
                success: function (data) {
                    $(data).each(function (index, item) {
                        titles.push(item);
                    });
                },
                error: function () {
                    alert('error');
                }
            });

            $("#tags").autocomplete({
                source: titles
            });
        }
    </script>


